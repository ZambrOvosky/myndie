
@{
    Layout = "~/Views/Shared/_DeveloperLayout.cshtml";
}
<link href="~/assets/css/entypo/css/entypo.css" rel="stylesheet" />
@foreach (var a in ViewBag.Apps)
{
    <div class="col-lg-12 mt-0">
        <div class="card">
            <div class="card-header">
                <div class="title h3"><strong>@a.Name:</strong></div>
            </div>
            @if(a.Id != null)
            {
                @Html.Action("_ListReviewDev", "Review", new { Appid = a.Id })
            }
        </div>
    </div>
}

