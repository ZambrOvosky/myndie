    <script src="~/assets/js/www_subdomain/forms-validation.js"></script>
    <script src="~/assets/js/www_subdomain/front.js"></script>
    <!-- Bootstrap Select-->
    <script src="~/assets/vendor/bootstrap-select/js/www_subdomain/bootstrap-select.min.js"></script>
