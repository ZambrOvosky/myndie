<body class="text-white">
    <header class="header">
        <nav class="navbar navbar-expand-lg">
            <div class="container-fluid d-flex align-items-center justify-content-between">
                <div class="navbar-header">
                    <!-- Navbar Header--><a href="~/Home/Index" class="navbar-brand">
                        <div class="brand-text brand-big visible text-uppercase"><strong class="text-primary">Myndie</strong><strong>Dashboard</strong></div>
                        <div class="brand-text brand-sm"><strong class="text-primary">M</strong><strong>D</strong></div>
                    </a>
                    <!-- Sidebar Toggle Btn-->
                    <button class="sidebar-toggle"><i class="fa fa-long-arrow-left"></i></button>
                </div>
                <div class="right-menu list-inline no-margin-bottom">
                    <!-- Log out               -->
                    <div class="list-inline-item logout">
                        <a id="logout" href="~/User/Logout"
                           class="nav-link">Logout<i class="icon-logout"></i></a>
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <div class="d-flex align-items-stretch">
        <!-- Sidebar Navigation-->
        <nav id="sidebar">
            <!-- Sidebar Header-->
            <div class="sidebar-header d-flex align-items-center">
                <a href="~/User/ProfileView">
                    <div class="avatar">
                        @if (ViewBag.User.Picture != null && ViewBag.User.Picture != "")
                        {
                            <img src="@ViewBag.User.Picture" style="height: 50px; width: 50px"
                                 alt="..." class="img-fluid rounded-circle">
                        }
                        else
                        {
                            <img src="~/assets/img/www_subdomain/avatar-6.jpg"
                                 alt="..." class="img-fluid rounded-circle">
                        }

                    </div>
                </a>
                <div class="title">
                    <h1 class="h5">@ViewBag.User.Name</h1>
                    <p>Developer</p>
                </div>
            </div>
            <!-- Sidebar Navidation Menus--><span class="heading">Main</span>
            <ul class="list-unstyled">
                <li class="active"><a href="~/Developer/ProfileView"> <i class="icon-home"></i>Home</a></li>
                <li><a href="/Application/Register"> <i class="note-icon-magic"></i>Add Application</a></li>
                <li><a href="/Review/YourReviews"> <i class="note-icon-pencil"></i>Reviews</a></li>
                <li><a href="/Developer/Sales"> <i class="icon-chart"></i>Sales</a></li>
                <li><a href="/Developer/Summary"> <i class="icon-grid"></i>Application Summary</a></li>
            </ul>
        </nav>
        <!-- Sidebar Navigation end-->
        <div class="page-content">
            <div class="page-header">
                <div class="container-fluid">
                    <h2 class="h5 no-margin-bottom">Dashboard</h2>
                </div>
            </div>
            @RenderBody()
            <footer class="footer">
                <div class="footer__block block no-margin-bottom">
                    <div class="container-fluid text-center">
                        <p class="no-margin-bottom">2018 &copy; Vortex Cronos. </p>
                    </div>
                </div>
            </footer>
        </div>
    </div>

    @section Scripts{
        @Scripts.Render("~/bundles/jqueryval")
    }

    <!-- JavaScript files-->
    <script src="~/assets/vendor/jquery/www_subdomain/jquery.min.js"></script>
    <script src="~/assets/vendor/popper.js/umd/www_subdomain/popper.min.js"></script>
