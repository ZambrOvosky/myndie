    <!-- Custom stylesheet - for your changes-->
    <link rel="stylesheet" href="~/assets/css/www_subdomain/custom.css">
    <!-- Favicon-->
    <link rel="shortcut icon" href="~/assets/img/www_subdomain/favicon.ico">
    <!-- DataTables CSS-->
    <link rel="stylesheet" href="~/assets/vendor/datatables.net-bs4/css/www_subdomain/dataTables.bootstrap4.css">
    <link rel="stylesheet" href="~/assets/vendor/datatables.net-responsive-bs4/css/www_subdomain/responsive.bootstrap4.min.css">
    <!-- Summernote CSS -->
    <link rel="stylesheet" href="~/assets/vendor/summernote/www_subdomain/summernote-bs4.css">
    <!-- Bootstrap Select CSS-->
    <link rel="stylesheet" href="~/assets/vendor/bootstrap-select/css/www_subdomain/bootstrap-select.min.css">
    <!-- Bootstrap Touchspin CSS-->
    <link rel="stylesheet" href="~/assets/vendor/bootstrap-touchspin/www_subdomain/jquery.bootstrap-touchspin.min.css">
    <!-- Bootstrap Datepicker CSS-->
    <link rel="stylesheet" href="~/assets/vendor/bootstrap-datepicker/css/www_subdomain/bootstrap-datepicker3.css">
    <!-- Bootstrap Tags input CSS-->
